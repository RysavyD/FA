@model _3F.Web.Models.Administration.UserWithSymbol

@{
    Layout = "~/Views/Shared/_FullPageLayout.cshtml";
}

@using (Html.BeginForm())
{
                        @Html.AntiForgeryToken()
    <div class="form-horizontal">

        <div class="form-group">
            @Html.Label("Jméno uživatele ", htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(m => m.UserName)
                @Html.ValidationMessage("userName", "", new { @class = "text-danger" })
                @Model.Message
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Zkontrolovat VS" name="submit" class="btn btn-danger" />
                <input type="submit" value="Přidělit VS" name="submit" class="btn btn-danger" style="margin-left:15px;" />
            </div>
        </div>
    </div>
}

@Html.FormSubmitWaitDialog()
